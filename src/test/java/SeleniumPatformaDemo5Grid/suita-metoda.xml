<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Prima suita de testNG" verbose="1" >
    <test name="Primul test din testNG  .xml file" >
        <classes>
            <class name="com.first.pachet.SeleniumAnnotations">
                <methods>
                    <exclude name="test1"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Al doilea test din testNG  .xml file" >
        <classes>
            <class name="com.first.pachet.SeleniumAnnotations">
                <methods>
                    <include name="test1"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
